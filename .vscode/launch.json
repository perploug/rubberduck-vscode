{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "run - app/vscode",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/app/vscode/dev"],
      "outFiles": ["${workspaceFolder}/lib/extension/dist/**/*.js"],
      "preLaunchTask": "build-extension"
    }
  ]
}
